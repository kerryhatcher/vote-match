VOTE_MATCH_DATABASE_URL=postgresql+psycopg://vote_match:vote_match_dev@localhost:35432/vote_match
VOTE_MATCH_LOG_LEVEL=INFO
VOTE_MATCH_LOG_FILE=logs/vote-match.log
VOTE_MATCH_DEFAULT_STATE=GA
VOTE_MATCH_DEFAULT_BATCH_SIZE=10000
VOTE_MATCH_CENSUS_TIMEOUT=300

VOTE_MATCH_GEOCODE_SERVICES__NOMINATIM__EMAIL=changeme@todo.com

# Geocodio (US/Canada only, Batch, Paid)
# Get API key from https://www.geocod.io/
VOTE_MATCH_GEOCODE_SERVICES__GEOCODIO__API_KEY=your_geocodio_key_here
VOTE_MATCH_GEOCODE_SERVICES__GEOCODIO__BATCH_SIZE=1000
VOTE_MATCH_GEOCODE_SERVICES__GEOCODIO__TIMEOUT=600

# Mapbox Geocoding API (Global, Batch, Paid)
# Get access token from https://account.mapbox.com/access-tokens/
VOTE_MATCH_GEOCODE_SERVICES__MAPBOX__API_KEY=your_mapbox_token_here
VOTE_MATCH_GEOCODE_SERVICES__MAPBOX__BATCH_SIZE=100
VOTE_MATCH_GEOCODE_SERVICES__MAPBOX__COUNTRY=us

# Photon (Global, Individual, Free)
# Public instance: https://photon.komoot.io
# Can also self-host: https://github.com/komoot/photon
VOTE_MATCH_GEOCODE_SERVICES__PHOTON__BASE_URL=https://photon.komoot.io
VOTE_MATCH_GEOCODE_SERVICES__PHOTON__RATE_LIMIT_DELAY=1.0

# Google Maps Geocoding API (Global, Individual, Paid)
# Get API key from https://console.cloud.google.com/
VOTE_MATCH_GEOCODE_SERVICES__GOOGLE__API_KEY=your_google_maps_key_here
VOTE_MATCH_GEOCODE_SERVICES__GOOGLE__REGION=us
VOTE_MATCH_GEOCODE_SERVICES__GOOGLE__RATE_LIMIT_DELAY=0.02

# USPS Address Validation API
# Get credentials from https://developer.usps.com/
VOTE_MATCH_USPS_CLIENT_ID=your_client_id_here
VOTE_MATCH_USPS_CLIENT_SECRET=your_client_secret_here
# Use production URL (default) or test URL for development
VOTE_MATCH_USPS_BASE_URL=https://apis.usps.com/addresses/v3
# For testing, use: https://apis-tem.usps.com/addresses/v3
VOTE_MATCH_USPS_TIMEOUT=60

# Cloudflare R2 Object Storage
# Documentation: https://developers.cloudflare.com/r2/api/tokens/
VOTE_MATCH_R2_ENABLED=false

# How to get R2 API Token credentials:
# 1. Go to Cloudflare Dashboard > R2 > Manage R2 API Tokens
# 2. Click "Create API Token"
# 3. Set permissions (Admin Read & Write or Object Read & Write)
# 4. (Optional) Scope to specific bucket
# 5. Copy the Access Key ID and Secret Access Key
# Note: Secret Access Key is only shown once - save it securely!

# R2 endpoint URL (replace <account-id> with your Cloudflare account ID)
VOTE_MATCH_R2_ENDPOINT_URL=https://your-account-id.r2.cloudflarestorage.com

# R2 API Token credentials (created in step above)
VOTE_MATCH_R2_ACCESS_KEY_ID=your_r2_api_token_access_key_id
VOTE_MATCH_R2_SECRET_ACCESS_KEY=your_r2_api_token_secret_access_key

# R2 bucket name (create bucket first in Cloudflare Dashboard)
VOTE_MATCH_R2_BUCKET_NAME=your-bucket-name

# Public URL for accessing uploaded maps
# Option 1: Custom domain (recommended) - https://maps.yourdomain.com
# Option 2: R2.dev subdomain - https://pub-xxxxxx.r2.dev
VOTE_MATCH_R2_PUBLIC_URL=https://your-public-url.r2.dev
